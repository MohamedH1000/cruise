{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "https://cruise-mu.vercel.app"
        }
      ],
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/stripe-webhook",
      "dest": "/api/stripe-webhook",
      "methods": ["POST"],
      "headers": {
        "content-type": "application/json"
      },
      "config": {
        "bodyParser": {
          "enabled": false
        }
      }
    }
  ]
}
